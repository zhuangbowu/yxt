.content {
  width: 100%;
  height: 80rpx;
}
.contentList {
  width: calc(100% - 20rpx);
  margin: 10rpx 10rpx;
  border-radius: 20rpx;
  border: 2rpx solid #999;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.contentList-image {
  width: 150rpx;
  height:150rpx;
  border-radius: 20rpx;
  margin-left: 40rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.contentList-right {
  width: calc(100% - 260rpx);
  margin-left: 20rpx;
}
.contentList-right>view:nth-child(1){
  width: calc(100% - 80rpx);
  margin-right: 80rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 32rpx;
  color: #333;
  line-height: 80rpx;
}
.contentList-right>view:nth-child(2){
  width: calc(100% - 80rpx);
  margin-right: 80rpx;
  font-size: 32rpx;
  color: red;
  line-height: 60rpx;
