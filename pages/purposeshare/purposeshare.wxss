.post {
  position: fixed;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 38rpx;
  color: #fff;
  background-color: red;
  font-weight: bold;
  text-align: center;
  left: 0rpx;
  bottom: 0rpx;
}
.navimg{
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  z-index: 1;
}
.content>view{
  position: relative;
  z-index: 2;
  background-color: #fff;
}
.msak{
  position: fixed;
  left: 0px;
  top: 0px;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.5);
  z-index: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.msak>image{
  width: 80%;
}