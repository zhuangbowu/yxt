.content {
  width: 100%;
  height: 80rpx;
}


.contentList {
  width: calc(100% - 20rpx);
  margin: 10rpx 10rpx;
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}

.contentList-image {
  width: 220rpx;
  height: 220rpx;
  border-radius: 20rpx;
  margin-left: 20rpx;
  margin-top: 100rpx;
  margin-bottom: 100rpx;
}

.contentList-right {
  width: calc(100% - 280rpx);
  margin-left: 20rpx;
}

.contentList-right>view:nth-child(1) {
  width: calc(100% - 80rpx);
  margin-right: 80rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 100rpx;
  line-height: 160rpx;
  font-size: 38rpx;
  color: #333;
}

.contentList-right>view:nth-child(2) {
  width: calc(100% - 80rpx);
  margin-right: 80rpx;
  font-size: 38rpx;
  color: red;
  line-height: 60rpx;
}

