page{
  background-color: #f1f1f1;
}
.data-list{
  background: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  width: calc(100% - 40rpx);
  padding: 0rpx 20rpx;
  border-bottom:2rpx solid #e2e2e2;
  line-height: 80rpx;
  font-size: 28rpx;
}
.data-list>view:nth-child(1){
  width: 80rpx;
}
.data-list>view:nth-child(2){
  width: 200rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.data-list>view:nth-child(3){
  width: 400rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.data-list>view:nth-child(4){
  width: 200rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.data-bak{
  margin-bottom: 100rpx;
}
.data-fixed{
  position: fixed;
  bottom: 0px;
  left: 0px;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.data-fixed>view{
  background-color: #ff9800;
  font-size: 32rpx;
  color:#fff;
  margin-left: 40%;
  width: 60%;
  text-align: center;
}
.checkbox-group{
  width: 40%;
  line-height: 100rpx;
  position: absolute;
  top:0rpx;
  left: 0rpx;
  background: #fff;
  text-align: center;
  display: block;
}