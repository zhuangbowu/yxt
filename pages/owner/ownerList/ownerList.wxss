/* pages/ownerList/ownerList.wxss */
page{
  background-color: #f1f1f1;
  width: 100%;
  min-height:100%; 
  color:#333;
}
.Order{
  position: fixed;
  top:0rpx;
  left: 0rpx;
  width: calc(100% - 38rpx);
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 20rpx;
  font-size: 28rpx;
  background-color: #f1f1f1;
  z-index: 2;
}
.Order>view:nth-child(1){
  position: relative;
  width: 500rpx;
  padding-left: 20rpx;
  border-radius: 8rpx;
  background-color: #fff;
  height: 60rpx;
  line-height: 60rpx;
}
.Order>view:nth-child(1) image{
  position: absolute;
  top:10rpx;
  right: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
.Order>view:nth-child(2){
  position: absolute;
  top:30rpx;
  right:40rpx;
}
.drop-down{
  position: fixed;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.mask{
  position: absolute;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
}
.drop-down-choice-seach{
  background-color: #e2e2e2;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  position: relative;
}
.drop-down-choice-seach>input{
  width: 560rpx;
  background-color: #fff;
  font-size: 28rpx;
  padding-top:10rpx;
  padding-bottom:10rpx;
  margin: 20rpx 40rpx 20rpx 0rpx;
  margin-left: 40rpx;
  padding-left: 20rpx;
  border-radius:60rpx;
}
.drop-down-choice-seach>view{
  position: absolute;
  font-size: 32rpx;
  color:#333;
  top:36rpx;
  right: 40rpx;
}
.drop-down-choice{
  position: absolute;
  top:100rpx;
  left: 0rpx;
  width: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.drop-down-choice>.views{
  width: 100%;
  height: 80rpx;
  line-height:80rpx;
  font-size: 32rpx;
  padding-left: 40rpx;
  border-bottom: 2rpx solid #e2e2e2;
}
.shopListContent{
  padding-top: 100rpx;
}
.shopList{
  border-bottom:8rpx solid #f1f1f1;
  background-color: #fff;
}
.shopList-one{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  line-height: 80rpx;
  padding: 0rpx 20rpx;
  color:#333;
}
.shopList-one>view{
  /* width: 50%; */
  font-size: 28rpx;
} 
.shopList-one>view:nth-child(1){
  width: calc(100% - 300rpx);
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; 
}
.shopList-one>view:nth-child(1)>.textone{
  color:aqua;
}
.shopList-one>view:nth-child(1)>.texttwo{
  color:#333;
}
.shopList-one>view:nth-child(2){
  text-align: right;
  width: 300rpx;
}
.shopList-two{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  line-height: 40rpx;
  padding: 0rpx 20rpx;
  color:#666;
  border-bottom: 2rpx solid #f1f1f1;
}
.shopList-two>view{
  /* width: 50%; */
  font-size: 28rpx;
}
.shopList-two>view:nth-child(odd){
  width: calc(100% - 200rpx);
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; 
}
.shopList-two>view:nth-child(even){
  text-align: right;
  width: 200rpx;
}
.footer{
  position: fixed;
  bottom:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  flex-grow: wrap;
  justify-content: space-around;
  text-align: center;
}
.footer>view{
  width: calc(100% / 3);
}
.footer>view>view{
  font-size: 28rpx;
  line-height: 28rpx;
}
.footer>view>image{
  width: 50rpx;
  height: 50rpx;
  padding-top: 10rpx;
}

.navgengduo{
  padding-bottom: 180rpx;
  font-size: 32rpx;
  color:#333;
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  width: 100%;
}
.refund{
  width: calc(100% - 40rpx);
  margin: 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.refund>view:nth-child(1){
  width: 50%;
  text-align: left;
  color:#333;
  font-size: 28rpx;
}
.refund>view:nth-child(2){
  width: 50%;
  text-align: right;
  font-size: 28rpx;
  color:#666;
}