.searchBlueTooth{
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #26b543;
  color:#fff;
}
.hasBluetooth{
  background-color: #fff;
  width: calc(100% - 40rpx);
  padding: 0rpx 20rpx;
  border-bottom: 2rpx solid #eee;
  line-height:98rpx;
  height:98rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.post{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  top:0rpx;
  left: 0rpx;
}
.checkbox{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  line-height: 100rpx;
  border-bottom: 2rpx solid #eee;
  color:#333;
  font-size: 28rpx;
}