page{
  background-color: #f1f1f1;
}
.group-header{
  box-sizing: border-box;
  width: 100%;
  height: 140rpx;
  position: relative;
  padding: 20rpx 10rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.group-header>image{
  width: 150rpx;
  border-radius:50%;
  position: absolute;
  top:20rpx;
  right: 20rpx; 
}
.group-header>view:nth-child(1)>image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.group-header>view:nth-child(2){
  padding-left: 20rpx;
}
.group-header>view:nth-child(2)>view:nth-child(1){
  padding-top: 20rpx;
  font-size: 32rpx;
  color:#333;
}
.group-header>view:nth-child(2)>view:nth-child(2){
  font-size: 28rpx;
  color:#666;
  padding-top: 10rpx;
}
.group-title{
  padding: 20rpx 0rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  border-bottom: 2rpx solid #e2e2e2;
  background-color: #fff;
}
.group-title>view:nth-child(1){
  padding: 6rpx 10rpx;
  border-radius: 4rpx;
  background-color:#44a0b7;
  color:#fff;
  font-size: 28rpx;
  margin-left: 20rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width:560rpx;
}
.group-title>view:nth-child(2){
  color:#333;
  font-size: 24rpx;
  padding-right: 20rpx;
}
.group-nocite{
  background-color: #fff;
  padding: 0rpx 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 6rpx solid #e2e2e2;
  position: relative;
  font-size: 32rpx;
  width: calc(100% - 40rpx);
  overflow: hidden;
}
.group-nocite>view:nth-child(1){
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  top:0rpx;
  left: 0rpx;
  z-index: 1;
  background-color: #fff;
}
.group-nocite>view:nth-child(1)>image{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top:20rpx;
  left: 20rpx;
}
.group-nocite>view:nth-child(2){
  width: calc(100% - 90rpx);
  overflow: hidden;
  white-space: nowrap;
  animation:myfirst 10s linear infinite;
}
@keyframes myfirst
{
  0% {margin-left:750rpx;}
  100% {margin-left:-1500rpx;}
}
.group-content{
  width: 100%;
  background-color: #fff;
}
.group-content-list{
  padding: 20rpx 20rpx;
  position: relative;
  height: 220rpx;
  border-bottom:1px solid #e2e2e2;
}
.group-content-list>image{
  width: 220rpx;
  height: 220rpx;
  position: absolute;
}
.group-content-list>video{
  width: 220rpx;
  height: 220rpx;
  position: absolute;
}
.group-content-list>view:nth-child(2){
  width: calc(100% - 240rpx);
  margin-left: 240rpx;
  height: 240rpx;
  position: relative;
}
.group-content-list>view:nth-child(2)>view{
  font-size: 36rpx;
  color:#333;
  line-height: 36rpx;
  font-weight: bold;
}
.group-content-list>.posi{
  position: absolute;
  top:40rpx;
  left: 20rpx;
  font-size: 30rpx;
  border-top-right-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
  color:#fff;
  line-height: 50rpx;
  padding: 0rpx 18rpx;
  /* background-color:#ffcd00; */
  background-color: #666;
}
.group-content-list>.posis{
  position: absolute;
  top:40rpx;
  left: 20rpx;
  font-size: 30rpx;
  border-top-right-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
  color:#fff;
  line-height: 50rpx;
  padding: 0rpx 18rpx;
  background-color:#ffcd00;
  /* background-color: #666; */
}

.group-content-list>view:nth-child(2)>view:nth-child(1){
  /* white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; */
}
.group-content-list>view:nth-child(2)>view:nth-child(2){
  position: absolute;
  left: 0rpx;
  bottom: 20rpx;
  color:#999;
  font-size: 40rpx;
  font-style: italic;
  font-weight: bold;
}
.group-content-list>view:nth-child(2)>view:nth-child(2){
  width: 100%;

  /* text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical; */
}
.group-content-list>view:nth-child(2)>view:nth-child(6){
  position: absolute;
  bottom: 0rpx;
  right: 20rpx;
  color:#44a0b7;
  font-size: 32rpx;
}
.group-fixed{
  position: fixed;
  bottom:0rpx;
  left: 0rpx;
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  display: flex;
  background-color: #fff;
  justify-content: space-around;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 36rpx;
}
.group-fixed>view{
  width: 50%;
}
.group-fixed>view:nth-child(1){
  background-color:#44a0b7;
  color: #fff;
}
.group-fixed>form:nth-child(2){
  width: 50%;
}
.group-fixed button{
  background-color:#ffcd00;
  color: #fff;
  width: 100%;
  border-radius: 0rpx;
  font-size: 32rpx;
  line-height: 100rpx;
}
.group-modify{
  position: absolute;
  bottom:0rpx;
  right: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.group-modify>image:nth-child(1){
  width: 64rpx;
  height: 64rpx;
}
.group-modify>view:nth-child(2){
  text-align: center;
  line-height: 64rpx;
  font-size: 36rpx;
}
.group-modify>image:nth-child(3){
  width: 64rpx;
  height: 64rpx;
}
.group-cot{
  border-top:8rpx solid #e2e2e2;
  padding: 20rpx 0rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  background-color: #fff;
}
.group-cot>view{
  width: calc(100% / 3);
  text-align: center;
}
.group-cot>view>view:nth-child(1){
  font-size: 32rpx;
  color:#333;
  line-height:50rpx;
}
.group-cot>view>view:nth-child(2){
  font-size: 32rpx;
  color:#333;
  line-height: 50rpx;
}
.record-title{
  font-size: 28rpx;
  color:#333;
  line-height: 60rpx;
  padding-left: 20rpx;
}
.record-content{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  width: 100%;
  background-color: #fff;
}
.record-content>view{
  line-height: 60rpx;
  font-size: 28rpx;
  color:#333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.record-content>view:nth-child(1){
