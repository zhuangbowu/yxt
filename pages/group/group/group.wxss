.search{
  width: 100%;
  height: 80rpx;
  padding: 30rpx 0rpx;
  background-color: #f1f1f1;
  display: flex;
  flex-grow:grow;
  flex-direction: row;
  position: relative;
}
.search>input{
  box-sizing: border-box;
  width:calc(100% - 140rpx);
  height: 80rpx;
  line-height:80rpx;
  background-color: #fff;
  margin-left: 20rpx;
  font-size: 32rpx;
  padding-left: 70rpx;
  border-radius: 10rpx;
}
.search>icon{
  position: absolute;
  top:50rpx;
  left: 40rpx;
}
.search>text{
  position: absolute;
  top:30rpx;
  right: 20rpx;
  font-size: 32rpx;
  line-height: 80rpx;
  padding:0rpx 20rpx;
  color:#1BC627;
}
.search>view{
  box-sizing: border-box;
  width:calc(100% - 40rpx);
  height: 80rpx;
  line-height:80rpx;
  background-color: #fff;
  margin-left: 20rpx;
  font-size: 32rpx;
  padding-left: 70rpx;
  border-radius: 10rpx;
  color:#999;
}
.owner-content{
  box-sizing: border-box;
  width: 100%;
  padding: 0rpx 20rpx;
}
.owner-content>view{
  border-bottom:2rpx solid #e2e2e2; 
  padding-bottom: 40rpx;
}
.owner-cont-header{
  margin-top: 20rpx;
  width: 100%;
  height: 100rpx;
}
.owner-cont-header>image{
  float: left;
  width: 60rpx;
  height: 60rpx;
  display: block;
  margin-top: 20rpx;
  border-radius:50%;
  border:2rpx solid #000;
}
.owner-cont-header>text{
  float: left;
  display: block;
  color:#333;
  font-size: 36rpx;
  padding-left: 10rpx;
  line-height: 100rpx;
}
.owner-cont-content1{
  border:1rpx solid #F6AB33;
  border-radius: 5rpx;
  margin-left: 100rpx;
}
.owner-cont-content2{
  border:2rpx solid #18D818;
  border-radius: 10rpx;
  margin-left: 100rpx;
}
.owner-cont-content3{
  border:2rpx solid #B7B7B7;
  border-radius: 10rpx;
  margin-left: 100rpx;
}
.owner-cont-cont-title{
  display: flex;
  flex-grow: grow;
  flex-direction: row;
  justify-content: left;
  position: relative;
}
.owner-cont-cont-title>view:nth-child(1){
  text-align: left;
  padding-left: 20rpx;
  color:#333;
  line-height: 80rpx;
  font-size: 32rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width:280rpx;
}
.owner-cont-cont-title>view:nth-child(2){
  text-align: right;
  padding-left:20rpx;
  color:#333;
  line-height: 80rpx;
  font-size: 32rpx;
}
.owner-cont-cont-title>image{
  width: 180rpx;
  position: absolute;
  bottom:-22rpx;
  right:-14rpx;
}
.owner-cont-cont-num2{
  border-top:2rpx solid #7cd69a;
}
.owner-cont-cont-num3{
  border-top:2rpx solid #B7B7B7;
}
.owner-cont-cont-num{
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: space-between;
}
.owner-cont-cont-num>view:nth-child(1){
  margin-right: 20rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  align-content:flex-start;
}
.owner-cont-cont-num>view:nth-child(1)>image{
  width: 60rpx;
  height: 60rpx;
  padding-top: 10rpx;
  margin-left: 10rpx;
}
.owner-cont-cont-num>view:nth-child(2){
  text-align: right;
  line-height: 80rpx;
  padding-right:20rpx;
  min-width:240rpx; 
  white-space: nowrap;
  font-size: 28rpx;
}
.owner-cont-img{
  display: flex;
  flex-wrap: wrap;
  margin-left: 100rpx;
}
.owner-cont-img>view{
  width: 137rpx;
  height:137rpx;
  margin-right: 16rpx;
  margin-top: 16rpx;
  position: relative;
}
.owner-cont-img>view>text{
  text-align: center;
  position: absolute;
  bottom: -2rpx;
  left: 2rpx;
  width: 100%;
  height: 35rpx;
  font-size: 26rpx;
  background-color: rgba(0, 0, 0, 0.6);
  color:#fff;
  line-height: 35rpx;
}
.owner-cont-img>view>image{
  border:2rpx solid #e2e2e2;
  width: 100%;
  height: 100%;
}
.owner-cont-img>view>video{
  border:2rpx solid #e2e2e2;
  width: 100%;
  height: 100%;
}
.owner-fixed{
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  bottom: 120rpx;
  right: 20rpx;
  background-color: #fff;
  border-radius:50%;
}
.owner-fixed>image{
  width: 80rpx;
  height: 80rpx;
}
.footer{
  position: fixed;
  bottom:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  flex-grow: wrap;
  justify-content: space-around;
  text-align: center;
}
.footer>view{
  width: calc(100% / 3);
}
.footer>view>view{
  font-size: 28rpx;
  line-height: 28rpx;
}
.footer>view>image{
  width: 50rpx;
  height: 50rpx;
  padding-top: 10rpx;
}
.owner-cont-time{
  margin-left: 100rpx;
  font-size: 36rpx;
  color:#333;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
}
.navgengduo{
  padding-bottom: 180rpx;
  font-size: 32rpx;
  color:#333;
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  width: 100%;
}