.nav{
  width:100%;
  height:100rpx;
  display:flex;
  flex-direction:row;
  background-color: #fff;
  position: relative;
  z-index:5;
}
.default{
  line-height:100rpx;
  text-align:center;
  flex:1;
  border-right:1px solid gainsboro;
  color:#000;
}
.red{
  line-height:100rpx;
  text-align:center;
  color:#fc5558;
  flex:1;
  border-right:1px solid gainsboro;
}
.show{
  display:block;
  padding-bottom: 120rpx;
}
.hidden{
  display:none;
}
.Order{
  position: relative;
  top:0rpx;
  left: 0rpx;
  width: calc(100% - 38rpx);
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 20rpx;
  font-size: 28rpx;
  background-color: #f1f1f1;
  z-index: 2;
}
.Order>view:nth-child(1){
  position: relative;
  width: 500rpx;
  padding-left: 20rpx;
  border-radius: 8rpx;
  background-color: #fff;
  height: 60rpx;
  line-height: 60rpx;
}
.Order>view:nth-child(1) image{
  position: absolute;
  top:10rpx;
  right: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
.Order>view:nth-child(2){
  position: absolute;
  top:30rpx;
  right:40rpx;
}
.drop-down{
  position: fixed;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.mask{
  position: absolute;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
}
.drop-down-choice-seach{
  background-color: #e2e2e2;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  position: relative;
}
.drop-down-choice-seach>input{
  width: 560rpx;
  background-color: #fff;
  font-size: 28rpx;
  padding-top:10rpx;
  padding-bottom:10rpx;
  margin: 20rpx 40rpx 20rpx 0rpx;
  margin-left: 40rpx;
  padding-left: 20rpx;
  border-radius:60rpx;
}
.drop-down-choice-seach>view{
  position: absolute;
  font-size: 32rpx;
  color:#333;
  top:36rpx;
  right: 40rpx;
}
.drop-down-choice{
  position: absolute;
  top:200rpx;
  left: 0rpx;
  width: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.drop-down-choice>.views{
  width: 100%;
  height: 80rpx;
  line-height:80rpx;
  font-size: 32rpx;
  padding-left: 40rpx;
  border-bottom: 2rpx solid #e2e2e2;
}
.league-embers{
  border-bottom: 10rpx solid #e2e2e2;
  width: 100%;
}
.league-embers>view:nth-child(1){
  padding: 0rpx 20rpx;
  width: calc(100% - 40rpx);
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  font-size: 28rpx;
  color: #333;
  line-height: 80rpx;
}
.league-embers>view:nth-child(1)>view:nth-child(1){
  width: 50%;
  text-align: left;
}
.league-embers>view:nth-child(1)>view:nth-child(1)>text{
  padding: 5rpx 10rpx;
  background-color: #e2e2e2;
  border-radius: 3rpx;
}
.league-embers>view:nth-child(1)>view:nth-child(2){
  text-align: right;
  width: 50%;
}
.league-content{
  width: calc(100% - 40rpx);
  padding: 20rpx 20rpx;
  border:2rpx solid #e2e2e2;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.league-content>view:nth-child(1){
  width: 140rpx;
}
.league-content>view:nth-child(1)>image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 70rpx;
}
.league-content>view:nth-child(2){
  margin-left: 20rpx;
  width: 550rpx;
}
.league-content>view:nth-child(2)>view:nth-child(1){
  border-bottom: 2rpx solid #e2e2e2;
  position: relative;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  text-align: left;
}
.league-content>view:nth-child(2)>view:nth-child(1)>.images{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top:20rpx;
  right: 0rpx;
}
.league-content>view:nth-child(2)>view:nth-child(1)>.imagess{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top:20rpx;
  right: 80rpx;
}
.league-content>view:nth-child(2)>view:nth-child(2){
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 28rpx;
  color:#333;
  padding-top: 20rpx;
  line-height: 80rpx
}
.league-content>view:nth-child(2)>view:nth-child(2)>view{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 100%;
  margin: 6rpx 0rpx;
}
.league-content>view:nth-child(2)>view:nth-child(2)>view>view:nth-child(1){
  position: relative;
  width: 50%;
  text-align: left;
}
.league-content>view:nth-child(2)>view:nth-child(2)>view>view:nth-child(1)>view{
  width: 100%;
  text-align: left;
  /* white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;  */
}
.league-content>view:nth-child(2)>view:nth-child(2)>view>view:nth-child(1)>text{
  position: absolute;
  top:0rpx;
  right: -100rpx;
  padding: 0rpx 10rpx;
  background-color: #e2e2e2;
  border-radius: 3rpx;
  margin-left: 40rpx;
}
.league-content>view:nth-child(2)>view:nth-child(2)>view>view:nth-child(2){
  width: 50%;
  text-align: right;
}
.league-address{
  padding: 0rpx 20rpx;
  font-size: 28rpx;
  color:#333;
  line-height: 80rpx;
  white-space:nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.league-goods{
  border-bottom: 10rpx solid #e2e2e2;
  width: calc(100% - 40rpx);
  padding: 0rpx 20rpx;
}
.league-goods>view{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 28rpx;
  line-height: 80rpx;
  color:#333;
}
.league-goods>view:nth-child(2){
  border-bottom: 2rpx solid #e2e2e2;
}
.league-goods>view>view{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.league-goods>view>view>view:nth-child(odd){
  width: 50%;
  text-align: left;
}
.league-goods>view>view>view:nth-child(even){
  width: 50%;
  text-align: right;
}
.league-goods>view>view>view:nth-child(even)>text{
  padding: 8rpx 14rpx;
  background-color: #e2e2e2;
  border-radius: 3rpx;
}
.league-goods>.goods-fot{
  text-align: right;
  color:#666;
  width: 100%;
  display: block;
}
.footer{
  position: fixed;
  bottom:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  flex-grow: wrap;
  justify-content: space-around;
  text-align: center;
}
.footer>view{
  width: calc(100% / 3);
}
.footer>view>view{
  font-size: 28rpx;
  line-height: 28rpx;
}
.footer>view>image{
  width: 50rpx;
  height: 50rpx;
  padding-top: 10rpx;
}
.league-goods2{
  border-bottom: 10rpx solid #e2e2e2;
  width: calc(100% - 40rpx);
  padding: 0rpx 20rpx;
}
.league-goods2>view{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 28rpx;
  line-height:60rpx;
  color:#333;
}
.league-goods2>view:nth-child(1)>view:nth-child(1){
  width: 50%;
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; 
}
.league-goods2>view:nth-child(1)>view:nth-child(2){
  width: 50%;
  text-align: right;
}
.league-goods2>view:nth-child(3)>view:nth-child(1){
  width: 50%;
  text-align: left;
}
.league-goods2>view:nth-child(3)>view:nth-child(2){
  width: 50%;
  text-align: right;
}
.league-goods2>view:nth-child(2)>view{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  border-bottom:2rpx solid #e2e2e2;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; 
}
.league-goods2>view:nth-child(2)>view>view:nth-child(1){
  width: 50%;
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; 
}
.league-goods2>view:nth-child(2)>view>view:nth-child(2){
  width: 50%;
  text-align: right;
}
.league-goods2>view:nth-child(4){
  width: 100%;
  text-align: right;
  font-size: 26rpx;
  color:#666;
}
.navgengduo{
  padding-bottom: 180rpx;
  font-size: 32rpx;
  color:#333;
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  width: 100%;
}