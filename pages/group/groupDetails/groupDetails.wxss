page{
  background-color: #f1f1f1;
}
.group-header{
  box-sizing: border-box;
  width: 100%;
  height: 140rpx;
  position: relative;
  padding: 20rpx 20rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.group-header>image{
  width: 150rpx;
  border-radius:50%;
  position: absolute;
  top:20rpx;
  right: 20rpx; 
}
.group-header>view:nth-child(1)>image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.group-header>view:nth-child(2){
  padding-left: 20rpx;
}
.group-header>view:nth-child(2)>view:nth-child(1){
  padding-top: 20rpx;
  font-size: 32rpx;
  color:#333;
}
.group-header>view:nth-child(2)>view:nth-child(2){
  font-size: 28rpx;
  color:#666;
  padding-top: 10rpx;
}
.group-title{
  padding: 20rpx 0rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  border-bottom: 1rpx solid #e2e2e2;
  background-color: #fff;
}
.group-title>view:nth-child(1){
  padding: 6rpx 10rpx;
  border-radius: 4rpx;
  background-color:#44a0b7;
  color:#fff;
  font-size: 28rpx;
  margin-left: 20rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width:560rpx;
}
.group-title>view:nth-child(2){
  color:#333;
  font-size: 24rpx;
  padding-right: 20rpx;
}
.group-nocite{
  background-color: #fff;
  padding: 0rpx 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 6rpx solid #e2e2e2;
  position: relative;
  font-size: 32rpx;
}
.group-nocite>view:nth-child(1){
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  top:0rpx;
  left: 0rpx;
  z-index: 1;
  background-color: #fff;
}
.group-nocite>view:nth-child(1)>image{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top:20rpx;
  left: 20rpx;
}
.group-nocite>view:nth-child(2){
  width: calc(100% - 90rpx);
  overflow: hidden;
  white-space: nowrap;
  animation:myfirst 10s linear infinite;
}
@keyframes myfirst
{
  0% {margin-left:750rpx;}
  100% {margin-left:-1500rpx;}
}
.group-amount{
  background-color: #fff;
  height: 120rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  border-bottom: 2rpx solid #f1f1f1;
}
.group-amount>view{
  box-sizing: border-box;
  padding-top: 20rpx;
  width: calc(100% / 3);
  text-align: center;
  border-right: 2rpx solid #f1f1f1;
}
.group-amount>view view{
  font-size: 28rpx;
  color:#333;
  line-height: 48rpx;
}
.group-content{
  width: 100%;
  background-color: #fff;
  padding-bottom: 120rpx;
}
.group-content-list{
  padding: 20rpx 20rpx;
  position: relative;
  border-bottom: 1px solid #e2e2e2;
}
.group-content-list>image{
  width: 220rpx;
  height: 220rpx;
  position: absolute;
}
.group-content-list>video{
  width: 220rpx;
  height: 220rpx;
  position: absolute;
}
.group-content-list>view:nth-child(2){
  width: calc(100% - 240rpx);
  margin-left: 240rpx;
  position: relative;
}
.group-content-list>view:nth-child(2)>view:nth-child(1){
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.group-content-list>view:nth-child(2)>view{
  font-size: 28rpx;
  color:#333;
  line-height: 44rpx;
}
.group-content-list>view:nth-child(2)>view>text{
  font-size: 34rpx;
  color:#999;
  display: inline-block;
  margin-left: 20rpx;
  text-decoration:line-through;
}
.group-content-list>view:nth-child(2)>view:nth-child(2){
  width: 100%;
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}
.group-fixed{
  position: fixed;
  bottom:0rpx;
  left: 0rpx;
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  display: flex;
  background-color: #fff;
  justify-content: space-around;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 34rpx;
}
.group-fixed>.btn1{
  width: calc(100% / 3);
  font-size: 34rpx;
  line-height: 100rpx;
}
.group-fixed>.btn2{
  width:100%;
  font-size: 34rpx;
  line-height: 100rpx;
  background-color:#ffcd00;
  color: #fff;
  border-radius:0rpx;
}
.group-fixed>.btn3{
  width:50%;
  font-size: 34rpx;
  line-height: 100rpx;
  background-color:#44a0b7;
  color: #fff;
  border-radius:0rpx;
}
.group-fixed>view{
  width: calc(100% / 3);
}
.group-fixed>view:nth-child(1){
  color:#44a0b7;
  background-color: #fff;
}
.group-fixed>view:nth-child(2){
  background-color:#44a0b7;
  color: #fff;
}
.group-fixed>button:nth-child(3){
  background-color:#ffcd00;
  color: #fff;
  border-radius:0rpx;
}
.group-fixed2{
  position: fixed;
  bottom:0rpx;
  left: 0rpx;
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  display: flex;
  background-color: #fff;
  justify-content: space-around;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 34rpx;
}
.group-fixed2>view{
  width: 50%;
}
.group-fixed2>view:nth-child(1){
  color:#fff;
  background-color: #ffcd00;
}
.group-fixed2>view:nth-child(2){
  background-color:#44a0b7;
  color: #fff;
}
.docc{
  text-decoration:line-through;
  color:#999;
  font-size: 28rpx;
}
.post{
  position: fixed;
  width: 120rpx;
  height: 120rpx;
  bottom: 200rpx;
  left: 20rpx;
  border-radius: 60rpx;
}
.post>image{
  width: 100%;
  height: 100%;
}